{
  "name": "web-component-define",
  "version": "3.0.0",
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./src/exports/index.js",
      "types": "./types/exports/index.d.ts"
    },
    "./define": {
      "import": "./src/exports/define.js",
      "types": "./types/exports/define.d.ts"
    },
    "./lit": {
      "import": "./src/exports/lit.js",
      "types": "./types/exports/lit.d.ts"
    },
    "./scoped-elements": {
      "import": "./src/exports/scoped-elements.js",
      "types": "./types/exports/scoped-elements.d.ts"
    },
    "./render-root": {
      "import": "./src/exports/render-root.js",
      "types": "./types/exports/render-root.d.ts"
    }
  },
  "files": [
    "types",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublish": "npm run build",
    "deploy": "npm run prepublish && npm publish",
    "test": "web-test-runner",
    "types:copy": "copyfiles \"./src/**/*.d.ts\" \"./*.d.ts\" types"
  },
  "keywords": [
    "web-components"
  ],
  "license": "MIT",
  "repository": "git@github.com:KonnorRogers/web-component-define.git",
  "bugs": {
    "url": "https://github.com/KonnorRogers/web-component-define/issues"
  },
  "homepage": "https://github.com/KonnorRogers/web-component-define",
  "author": "KonnorRogers <konnor5456@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@open-wc/testing": "^3.1.8",
    "@types/mocha": "^10.0.1",
    "@web/test-runner": "^0.16.1",
    "@web/test-runner-playwright": "^0.10.0",
    "@webcomponents/scoped-custom-element-registry": "^0.0.9",
    "copyfiles": "^2.4.1",
    "lit": "^2.7.4",
    "playwright": "^1.34.3",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@lit/reactive-element": "^1.6.1",
    "@open-wc/dedupe-mixin": "^1.3.1"
  }
}
